{
  "name": "claims-dashboard",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently --names api,ui \"bun run dev:server\" \"bun run dev:client\"",
    "dev:all": "concurrently --names api,ui,orch \"bun run dev:server\" \"bun run dev:client\" \"bun run dev:orchestrator\"",
    "dev:server": "bun run --watch server/index.ts",
    "dev:client": "vite",
    "dev:orchestrator": "bun run --watch orchestrator/index.ts",
    "orchestrator": "bun run orchestrator/index.ts",
    "build": "vite build && bun build server/index.ts --outdir=dist/server --target=bun",
    "start": "bun run dist/server/index.js",
    "test": "bun test",
    "db:migrate": "bun run server/db/migrate.ts"
  },
  "dependencies": {
    "hono": "^4.0.0",
    "postgres": "^3.4.0",
    "zod": "^3.22.0",
    "nanoid": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.5.0",
    "@hello-pangea/dnd": "^16.6.0",
    "react-hot-toast": "^2.4.1"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  }
}
